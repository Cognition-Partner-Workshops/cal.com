<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Cal.com Selenium Test Suite" parallel="classes" thread-count="2">
    
    <parameter name="browser" value="chrome"/>
    
    <listeners>
        <listener class-name="org.testng.reporters.JUnitXMLReporter"/>
    </listeners>
    
    <test name="Array Utils Tests">
        <classes>
            <class name="com.calcom.tests.ArrayUtilsTest"/>
        </classes>
    </test>
    
    <test name="All Unit Tests">
        <packages>
            <package name="com.calcom.tests.*"/>
        </packages>
    </test>
    
</suite>
